		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^6.0.11",
		"wrangler": "^3.105.0"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "^0.37.4",
		"@auth/d1-adapter": "^1.7.4",
		"@auth/sveltekit": "1.7.4",
		"@fontsource/geist-sans": "^5.1.0",
		"@tanstack/table-core": "^8.20.5",
		"bits-ui": "^1.0.0-next.78",
		"clsx": "^2.1.1",
		"formsnap": "^2.0.0",
