		"@auth/sveltekit": "1.5.3",
		"@fontsource/geist-sans": "^5.1.0",
		"@tanstack/table-core": "^8.20.5",
		"bits-ui": "^1.0.0-next.72",
		"clsx": "^2.1.1",
